
.content {
  background: linear-gradient(180deg, #DBEAFF 0%, #F5F7FB 100%);
  height: 100%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.box {
  width: 710rpx;
  background-color: white;
  border-radius: 20rpx;
  margin-top: 20rpx;
  padding: 24rpx 30rpx 32rpx 30rpx;
  box-sizing: border-box;
}
.title {
  font-size: 32rpx;
  margin-bottom: 42rpx;
}
.ul {
  display: flex;
  flex-wrap: wrap;
  justify-content: left;
}
.li {
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  width: calc(100% / 4);
  font-size: 24rpx;
  margin-bottom: 22rpx;
  margin-top: 14rpx;
}
.li image {
  width: 66rpx;
  height: 66rpx;
  margin-bottom: 20rpx;
}

